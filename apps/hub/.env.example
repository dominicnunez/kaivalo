# Environment Variables for Kaivalo Hub
# Copy this file to .env and fill in values

# WorkOS AuthKit
WORKOS_CLIENT_ID=your_client_id_here
WORKOS_API_KEY=your_api_key_here
# Port must match your running server: 3100 for production (PM2), 5173 for `vite dev`
# For vite dev: http://localhost:5173/auth/callback
# Use https:// in production
WORKOS_REDIRECT_URI=http://localhost:3100/auth/callback
# 64 hex chars = 32 bytes of entropy. Generate with: openssl rand -hex 32
WORKOS_COOKIE_PASSWORD=your_64_char_hex_string_here
