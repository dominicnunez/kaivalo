# Environment Variables for Kaivalo Hub
# Copy this file to .env and fill in values

# WorkOS AuthKit
WORKOS_CLIENT_ID=your_client_id_here
WORKOS_API_KEY=your_api_key_here
# Port must match your running server: 5173 for `vite dev`, 3100 for production (PM2)
WORKOS_REDIRECT_URI=http://localhost:5173/auth/callback
WORKOS_COOKIE_PASSWORD=your_32_char_hex_string_here
